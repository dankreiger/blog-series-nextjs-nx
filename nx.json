{
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "affected": {
    "defaultBase": "master"
  },
  "npmScope": "juridev",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ],
        "parallel": 3
      }
    }
  },
  "projects": {
    "markdown": {
      "tags": []
    },
    "shared-mdx-elements": {
      "tags": []
    },
    "shared-ui": {
      "tags": []
    },
    "site": {
      "tags": [],
      "implicitDependencies": [
        "site-articles"
      ]
    },
    "site-e2e": {
      "tags": [],
      "implicitDependencies": [
        "site"
      ]
    },
    "storybook-e2e-ui-e2e": {
      "tags": [],
      "implicitDependencies": [
        "shared-ui"
      ]
    },
    "site-articles": {
      "tags": []
    },
    "reactui": {
      "tags": []
    },
    "react-app-e2e": {
      "tags": [],
      "implicitDependencies": [
        "react-app"
      ]
    },
    "react-app": {
      "tags": []
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "site",
  "cli": {
    "defaultCollection": "@nrwl/next"
  }
}